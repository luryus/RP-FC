{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "probe-rs-debug",
        "request": "launch",
        "name": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "probe-rs",
        "runtimeArgs": ["dap-server"],
        "chip": "RP2040",
        "flashingConfig": {
          "flashingEnabled": true,
          "haltAfterReset": false,
          "formatOptions": {
            "binaryFormat": "elf"
          }
        },
        "coreConfigs": [
          {
            "coreIndex": 0,
            "programBinary": "${workspaceFolder}/target/thumbv6m-none-eabi/debug/katanactl2",
            //"svdFile": "Relative or fully qualified path to the CMSIS-SVD file for your target core",
            "rttEnabled": true
          }
        ],
        "env": {
          //!MODIFY (or remove)
          // If you set this variable, check the VSCode console log window for the location of the log file.
          "RUST_LOG": "info"
        },
         // Info, Debug
        "consoleLogLevel": "Console"
      }
    ]
  }